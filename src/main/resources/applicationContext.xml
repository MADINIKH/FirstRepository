<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
     http://www.springframework.org/schema/context
     http://www.springframework.org/schema/context/spring-context-4.0.xsd">
     
     
     <bean id="a1" class="com.demo1.Address">
       <constructor-arg value="hyd"></constructor-arg>
       <constructor-arg value="Telangana"></constructor-arg>
       <constructor-arg value="India"></constructor-arg>
     </bean>
	
	
	<bean id = "e" class = "com.demo1.Employee" autowire="byName">
	<constructor-arg value = "10" type = "int"></constructor-arg>
	<constructor-arg value = "Nik"></constructor-arg>
	
	
	</bean>
	
</beans>